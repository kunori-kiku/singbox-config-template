{
    "log": {
        "disabled": false,
        "level": "warn",
        "output": "box.log",
        "timestamp": true
    },
    "ntp": {
        "enabled": true,
        "server": "ntp.aliyun.com",
        "server_port": 123,
        "interval": "30m",
        "detour": "direct-out"
    },
    "experimental": {
        "cache_file": {
            "enabled": true,
            "store_fakeip": true,
            "store_rdrc": false
        },
        "clash_api": {
            "external_controller": "127.0.0.1:9090",
            "external_ui": "dashboard",
            "external_ui_download_detour": "proxy",
            "default_mode": "Rule"
        }
    },
    "dns": {
        "fakeip": {
            "enabled": true,
            "inet4_range": "198.18.0.0/15",
            "inet6_range": "fc00::/18"
        },
        "servers": [
            {
                "tag": "fakeip",
                "address": "fakeip"
            },
            {
                "tag": "dns_direct",
                "address": "https://dns.alidns.com/dns-query",
                "address_resolver": "dns_resolver",
                "strategy": "ipv4_only",
                "detour": "direct-out"
            },
            {
                "tag": "dns_resolver",
                "address": "h3://223.5.5.5/dns-query",
                "detour": "direct-out"
            },
            {
                "tag": "dns_proxy",
                "address": "https://1.1.1.1/dns-query",
                "strategy": "ipv4_only",
                "detour": "proxy"
            },
            {
                "tag": "block",
                "address": "rcode://success"
            }
        ],
        "rules": [
            {
                "outbound": "any",
                "server": "dns_resolver"
            },
            {
                "rule_set": [
                    "reject_non_ip",
                    "reject_non_ip_no_drop",
                    "reject_domainset"
                ],
                "server": "block"
            },
            {
                "clash_mode": "direct",
                "server": "dns_direct"
            },
            {
                "clash_mode": "global",
                "query_type": [
                    4,
                    6
                ],
                "server": "fakeip"
            },
            {
                "clash_mode": "global",
                "server": "dns_proxy"
            },
            {
                "rule_set": [
                    "domestic_non_ip",
                    "apple_cn_non_ip",
                    "apple_services",
                    "apple_cdn"
                ],
                "server": "dns_direct"
            },
            {
                "rule_set": [
                    "geosite-bilibili",
                    "global_non_ip"
                ],
                "query_type": [
                    4,
                    6
                ],
                "server": "fakeip"
            },
            {
                "rule_set": "geosite-cn",
                "invert": true,
                "query_type": [
                    4,
                    6
                ],
                "server": "fakeip"
            }
        ],
        "strategy": "prefer_ipv4",
        "final": "dns_proxy"
    },
    "inbounds": [
        {
            "tag": "tun-in",
            "type": "tun",
            "interface_name": "singbox",
            "address": [
                "172.19.0.1/30",
                "fdfe:2952:8964::1/126"
            ],
            "stack": "mixed",
            "auto_route": true,
            "strict_route": true,
            "sniff": true
        }
    ],
    "outbounds": [
        {
            "tag": "direct-out",
            "type": "direct"
        },
        {
            "tag": "dns-out",
            "type": "dns"
        },
        {
            "tag": "block",
            "type": "block"
        },
        {
            "type": "urltest",
            "tag": "all-auto",
            "outbounds": [
            ],
            "url": "http://www.gstatic.com/generate_204",
            "interval": "5m",
            "tolerance": 50
        },
        {
            "tag": "hk",
            "type": "selector",
            "outbounds": [
                "direct-out"
            ],
            "default": 
        },
        {
            "tag": "tw",
            "type": "selector",
            "outbounds": [
                "direct-out"
            ],
            "default": 
        },
        {
            "tag": "proxy",
            "type": "selector",
            "outbounds": [
                "hk",
                "tw",
                "direct-out"
            ],
            "default": "hk"
        },
        {
            "tag": "apple",
            "type": "selector",
            "outbounds": [
                "proxy",
                "direct-out",
                "hk",
                "tw"
            ],
            "default": "direct-out"
        },
        {
            "tag": "telegram",
