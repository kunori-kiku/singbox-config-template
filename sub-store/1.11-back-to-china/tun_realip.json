{
    "log": {
        "disabled": false,
        "level": "warn",
        "timestamp": true
    },
    "ntp": {
        "enabled": true,
        "server": "ntp.org",
        "server_port": 123,
        "interval": "30m",
        "detour": "direct-out"
    },
    "experimental": {
        "cache_file": {
            "enabled": true,
            "store_fakeip": true,
            "store_rdrc": false
        },
        "clash_api": {
            "external_controller": "127.0.0.1:9090",
            "external_ui": "dashboard",
            "external_ui_download_url": "https://github.com/Zephyruso/zashboard/releases/latest/download/dist.zip",
            "external_ui_download_detour": "direct-out",
            "default_mode": "rule"
        }
    },
    "dns": {
        "fakeip": {
            "enabled": true,
            "inet4_range": "198.18.0.0/15",
            "inet6_range": "fc00:8964::/32"
        },
        "servers": [
            {
                "tag": "dns-direct",
                "address": "https://1.1.1.1/dns-query",
                "detour": "direct-out"
            },
            {
                "tag": "dns-proxy",
                "address": "https://1.1.1.1/dns-query",
                "detour": "proxy-other"
            },
            {
                "tag": "dns-cn",
                "address": "https://223.5.5.5/dns-query",
                "detour": "proxy-cn"
            },
            {
                "tag": "ads",
                "address": "fakeip"
            }
        ],
        "rules": [
            {
                "outbound": "any",
                "server": "dns-direct"
            },
            {
                "action": "route",
                "server": "ads",
                "query_type": [
                    "A",
                    "AAAA"
                ],
                "rule_set": [
                    "reject_non_ip",
                    "reject_non_ip_no_drop",
                    "reject_domainset",
                    "reject_supplement"
                ]
            },
            {
                "action": "route",
                "clash_mode": "direct",
                "server": "dns-direct"
            },
            {
                "action": "route",
                "clash_mode": "global",
                "server": "dns-cn"
            },
            {
                "action": "route",
                "server": "dns-proxy",
                "invert": true,
                "rule_set": [
                    "geoip-cn"
                ]
            },
            {
                "action": "route",
                "clash_mode": "rule",
                "server": "dns-cn"
            }
        ],
        "final": "dns-proxy"
    },
    "inbounds": [
        {
            "tag": "tun-in",
            "type": "tun",
            "interface_name": "singbox",
            "address": [
                "172.19.0.1/30",
                "fdfe:2952:8964::1/126"
            ],
            "stack": "mixed",
            "auto_route": true,
            "strict_route": true
        }
    ],
    "outbounds": [
        {
            "tag": "direct-out",
            "type": "direct"
        },
        {
            "tag": "proxy-other",
            "type": "selector",
            "outbounds": [
                "direct-out",
                "warp"
            ]
        },
        {
            "tag": "proxy-cn",
            "type": "selector",
            "outbounds": [
            ]
        }
    ],
    "route": {
        "rules": [
            {
                "type": "logical",
                "mode": "or",
                "rules": [
                    {
                        "protocol": "dns"
                    },
                    {
                        "port": 53
                    }
                ],
                "action": "hijack-dns"
            },
            {
                "clash_mode": "direct",
                "action": "route",
                "outbound": "direct-out"
            },
            {
                "clash_mode": "global",
                "action": "route",
                "outbound": "proxy-cn"
            },
            {
                "rule_set": [
                    "reject_non_ip",
                    "reject_non_ip_no_drop",
                    "reject_domainset",
                    "reject_supplement"
                ],
                "action": "reject"
            },
            {
                "action": "resolve"
            },
            {
                "rule_set": [
                    "reject_ip"
                ],
                "action": "reject"
            },
            {
                "rule_set": [
                    "domestic_ip",
                    "geoip-cn"
                ],
                "action": "route",
                "outbound": "proxy-cn"
            },
            {
                "rule_set": "lan_ip",
                "action": "route",
                "outbound": "direct-out"
            }
        ],
        "rule_set": [
            {
                "tag": "reject_non_ip_no_drop",
                "type": "remote",
                "format": "binary",
                "url": "https://github.com/kunori-kiku/sing-box-ruleset/raw/main/non_ip/reject-no-drop.srs",
                "download_detour": "direct-out"
            },
            {
                "tag": "reject_non_ip_drop",
                "type": "remote",
                "format": "binary",
                "url": "https://github.com/kunori-kiku/sing-box-ruleset/raw/main/non_ip/reject-drop.srs",
                "download_detour": "direct-out"
            },
            {
                "tag": "reject_non_ip",
                "type": "remote",
                "format": "binary",
                "url": "https://github.com/kunori-kiku/sing-box-ruleset/raw/main/non_ip/reject.srs",
                "download_detour": "direct-out"
            },
            {
                "tag": "reject_domainset",
                "type": "remote",
                "format": "binary",
                "url": "https://github.com/kunori-kiku/sing-box-ruleset/raw/main/domainset/reject.srs",
                "download_detour": "direct-out"
            },
            {
                "tag": "reject_supplement",
                "type": "remote",
                "format": "binary",
                "url": "https://github.com/kunori-kiku/sing-box-ruleset/raw/main/supplementary_rules/reject.srs",
                "download_detour": "direct-out"
            },
            {
                "tag": "reject_ip",
                "type": "remote",
                "format": "binary",
                "url": "https://github.com/kunori-kiku/sing-box-ruleset/raw/main/ip/reject.srs",
                "download_detour": "direct-out"
            },
            {
                "tag": "lan_non_ip",
                "type": "remote",
                "format": "binary",
                "url": "https://github.com/kunori-kiku/sing-box-ruleset/raw/main/non_ip/lan.srs",
                "download_detour": "direct-out"
            },
            {
                "tag": "lan_ip",
                "type": "remote",
                "format": "binary",
                "url": "https://github.com/kunori-kiku/sing-box-ruleset/raw/main/ip/lan.srs",
                "download_detour": "direct-out"
            },
            {
                "tag": "domestic_ip",
                "type": "remote",
                "format": "binary",
                "url": "https://github.com/kunori-kiku/sing-box-ruleset/raw/main/ip/domestic.srs",
                "download_detour": "direct-out"
            },
            {
                "tag": "geoip-cn",
                "type": "remote",
                "format": "binary",
                "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geoip/cn.srs",
                "download_detour": "direct-out"
            }
        ],
        "auto_detect_interface": true,
        "final": "proxy-other"
    }
}